{
  "name": "@backstage-community/plugin-ocm-backend",
  "version": "5.15.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "backend-plugin",
    "pluginId": "ocm",
    "pluginPackages": [
      "@backstage-community/plugin-ocm",
      "@backstage-community/plugin-ocm-backend",
      "@backstage-community/plugin-ocm-common"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "scripts": {
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "lint:check": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "postpack": "backstage-cli package postpack",
    "prepack": "backstage-cli package prepack",
    "start": "backstage-cli package start",
    "test": "NODE_OPTIONS='--no-node-snapshot --experimental-vm-modules' backstage-cli package test --passWithNoTests --coverage",
    "tsc": "tsc",
    "prettier:check": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write .",
    "build:api-docs": "./scripts/openapi-doc.sh",
    "build:api-server": "yarn backstage-repo-tools package schema openapi generate --server"
  },
  "configSchema": "config.d.ts",
  "dependencies": {
    "@backstage-community/plugin-ocm-common": "workspace:^",
    "@backstage/backend-defaults": "^0.15.1",
    "@backstage/backend-openapi-utils": "^0.6.5",
    "@backstage/backend-plugin-api": "^1.6.2",
    "@backstage/catalog-model": "^1.7.6",
    "@backstage/errors": "^1.2.7",
    "@backstage/plugin-catalog-node": "^1.20.1",
    "@backstage/plugin-permission-common": "^0.9.5",
    "@kubernetes/client-node": "1.4.0",
    "express": "^4.18.2",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@backstage/backend-dynamic-feature-service": "^0.7.8",
    "@backstage/backend-test-utils": "^1.10.4",
    "@backstage/cli": "^0.35.3",
    "@backstage/config": "^1.3.6",
    "@backstage/plugin-catalog-backend": "^3.3.2",
    "@openapitools/openapi-generator-cli": "2.28.2",
    "@types/express": "4.17.25",
    "@types/supertest": "6.0.3",
    "msw": "1.3.5",
    "supertest": "7.1.4"
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/backstage/community-plugins",
    "directory": "workspaces/ocm/plugins/ocm-backend"
  },
  "keywords": [
    "backstage",
    "plugin"
  ],
  "bugs": "https://github.com/backstage/community-plugins/issues",
  "maintainers": [
    "@04kash"
  ],
  "author": "Red Hat"
}
