    "@backstage/errors": "^1.2.4",
    "@backstage/integration": "^1.15.1",
    "@backstage/plugin-catalog-node": "^1.13.1",
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "leasot": "^12.0.0",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@backstage/backend-test-utils": "^1.0.2",
    "@backstage/cli": "^0.28.0",
