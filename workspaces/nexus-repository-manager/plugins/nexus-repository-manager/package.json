{
  "name": "@backstage-community/plugin-nexus-repository-manager",
  "version": "1.19.2",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha/index.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "nexus-repository-manager",
    "pluginPackage": "@backstage-community/plugin-nexus-repository-manager",
    "pluginPackages": [
      "@backstage-community/plugin-nexus-repository-manager"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "generate": "openapi-ts --input ./nexus-swagger.json --output ./src/generated",
    "lint": "backstage-cli package lint",
    "postpack": "backstage-cli package postpack",
    "prepack": "backstage-cli package prepack",
    "start": "backstage-cli package start --config ./dev/app-config.yaml",
    "start:alpha": "backstage-cli package start --entrypoint dev/alpha --config dev/alpha/app-config.yaml",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "tsc": "tsc"
  },
  "dependencies": {
    "@backstage/catalog-model": "^1.7.6",
    "@backstage/core-compat-api": "^0.5.5",
    "@backstage/core-components": "^0.18.4",
    "@backstage/core-plugin-api": "^1.12.1",
    "@backstage/frontend-plugin-api": "^0.13.2",
    "@backstage/plugin-catalog-react": "^1.21.4",
    "@backstage/theme": "^0.7.1",
    "@material-ui/core": "^4.9.13",
    "filesize": "^10.1.6",
    "luxon": "^3.6.1",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@backstage/cli": "^0.35.1",
    "@backstage/core-app-api": "^1.19.3",
    "@backstage/dev-utils": "^1.1.18",
    "@backstage/frontend-defaults": "^0.3.4",
    "@backstage/plugin-catalog": "^1.32.1",
    "@backstage/test-utils": "^1.7.14",
    "@backstage/ui": "^0.10.0",
    "@hey-api/openapi-ts": "0.89.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "cross-fetch": "4.1.0",
    "msw": "1.3.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "files": [
    "app-config.example.yaml",
    "config.d.ts",
    "dist"
  ],
  "configSchema": "config.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/backstage/community-plugins",
    "directory": "workspaces/nexus-repository-manager/plugins/nexus-repository-manager"
  },
  "keywords": [
    "backstage",
