  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "backstage",
    "grafana"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/backstage/community-plugins",
    "directory": "workspaces/grafana/plugins/grafana"
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "grafana",
    "pluginPackages": [
